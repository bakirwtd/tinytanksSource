local plr = "Tank-"..game.Players.LocalPlayer.Name
local sets = game.Workspace.Tanks[plr].Settings
local remotes = game.Workspace.Tanks[plr].Remotes
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TINYFUCKER", "Ocean")
local Tab = Window:NewTab("Main")
local Tab2 = Window:NewTab("Ammo Types")
local Section = Tab:NewSection("Main")
local Section2 = Tab2:NewSection("Bullet Type")
Section:NewButton("FireRate+INF Ammo", "Ez Minigun Lol", function()
    sets.MaxFireRate.Value = 0.05
    sets.LoadedShots.Value = 9999
end)
Section:NewButton("Speed", "Ez Speedy Boi lol", function()
    sets.RotationSpeed.Value = 9999
    sets.MoveSpeed.Value = 100
end)

Section2:NewButton("Normal Ammo", "no info", function()

    sets.LoadedAmmoType.Value = "Base"
    sets.LoadedAmmoTypeLocal.Value = "Base"
    remotes.LoadAmmoType:FireServer("Base")
    sets.LoadedShots.Value = 4

end)
Section2:NewButton("Explosive Ammo", "no info", function()

    sets.LoadedAmmoType.Value = "AoE"
    sets.LoadedAmmoTypeLocal.Value = "AoE"
    remotes.LoadAmmoType:FireServer("AoE")
    sets.LoadedShots.Value = 4

end)
Section2:NewButton("Piercing Ammo", "no info", function()

    sets.LoadedAmmoType.Value = "Piercing"
    sets.LoadedAmmoTypeLocal.Value = "Piercing"
    remotes.LoadAmmoType:FireServer("Piercing")
    sets.LoadedShots.Value = 4

end)
Section2:NewButton("Bouncy Ammo", "no info", function()

    sets.LoadedAmmoType.Value = "Bouncy"
    sets.LoadedAmmoTypeLocal.Value = "Bouncy"
    remotes.LoadAmmoType:FireServer("Bouncy")
    sets.LoadedShots.Value = 4

end)
Section2:NewButton("Rocket Ammo", "no info", function()

    sets.LoadedAmmoType.Value = "Rocket"
    sets.LoadedAmmoTypeLocal.Value = "Rocket"
    remotes.LoadAmmoType:FireServer("Rocket")
    sets.LoadedShots.Value = 4

end)
